{
  "manifest_version": 3,
  "name": "Harvest by Data Rush",
  "version": "1.0",
  "description": "Add tweets to your dataset with predefined or custom tags.",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.x.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
